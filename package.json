{
  "name": "react-native-searchbar-controlled",
  "version": "2.0.0",
  "description": "A pretty awesome search bar for React Native (controlled)",
  "keywords": [
    "react-native",
    "search",
    "bar",
    "searchbar",
    "animated",
    "ios",
    "android"
  ],
  "files": [
    "LICENSE.md",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "p": "prettier --write *.js *.md",
    "fix": "npm run lint -- --fix",
    "lint": "eslint index.js",
    "prepublishOnly": "npm run test",
    "test": "npm run lint"
  },
  "author": {
    "name": "Dave Olsen",
    "email": "do@localz.co",
    "url": "https://github.com/daveols"
  },
  "contributors": [
    "Hawken Rives <hawkrives@gmail.com>"
  ],
  "repository": "git://github.com/hawkrives/react-native-searchbar-controlled.git",
  "license": "MIT",
  "bugs": "https://github.com/hawkrives/react-native-searchbar-controlled/issues",
  "homepage": "https://github.com/hawkrives/react-native-searchbar-controlled#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.1.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.5.10"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react-native/all"
    ],
    "plugins": [
      "prettier",
      "react",
      "react-native"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react-native/no-inline-styles": 0
    }
  }
}
